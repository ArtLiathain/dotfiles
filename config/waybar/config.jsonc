        "mpris": {
			"player": "spotify_player",	
			"format": "{status_icon} {title} - {artist}",
			"format-stopped": "{status_icon}",
			"artist-len": 15,
			"title-len": 20,
			"player-icons": {
				"firefox": "Ôâ©",
				"chromium": "ÔÄÅ"
			},
			"status-icons": {
				"playing": "ÔÖÑ ",
				"paused": "Ôäã ",
				"stopped": "Ôäç "
			},
			"ignored-players": ["firefox"]
		},
		"group/audio":
		{
            "orientation": "inherit",
			"modules": [
					"pulseaudio#input", "pulseaudio#output"
			]	
		},
		"custom/perf-icon": {
			"tooltip": false,	
			"format": "Ó¥Ø"
		},
        "custom/separator": {
            "format": "|",
            "tooltip": false
        },
            "custom/arch-icon": {
                "tooltip": true,
                "format": " ",
                "tooltip-format": "Arch Linux"
        },
            "group/connection":
		{
			"orientation": "inherit",
			"modules": [
				"bluetooth", "network"
			]
		},
        "clock": {
            "format": "{:L%A %d - %I:%M %p}",
            "format-alt": "{:L%I:%M:%S %p}",
            "interval": 1,   
            "tooltip-format": "<tt>{calendar}</tt>",
            "smooth-scrolling-threshold": 0.01,
            "calendar": {
                "mode"          : "year",
                "mode-mon-col"  : 3,
                "weeks-pos"     : "right",
                "on-scroll"     : 1,
                "format": {
                    "months":     "<span color='#ffead3'><b>{}</b></span>",
                    "days":       "<span color='#ecc6d9'><b>{}</b></span>",
                    "weeks":      "<span color='#99ffdd'><b>W{}</b></span>",
                    "weekdays":   "<span color='#ffcc66'><b>{}</b></span>",
                    "today":      "<span color='#ff6699'><b><u>{}</u></b></span>"
                }
            },
            "actions":  {
                    "on-click-right": "mode",
                    "on-scroll-up": "shift_up",
                    "on-scroll-down": "shift_down"
            }
		},
        "network": {
            "format-wifi": "Ôá´ ",
            "format-ethernet":"ÓΩÑ‚Äâ",
            "format-disconnected": "Ó¨Å",
            "tooltip-format-disconnected": "Not connected",
            "tooltip-format-wifi": "{essid} ({signalStrength}%) Ôá´",
            "tooltip-format-ethernet": "{ifname} üñß  ÔÉò {bandwidthUpBits} ÔÉó {bandwidthDownBits}",
            "on-click": "wezterm start impala"
        },
        "bluetooth": {
            "format": "Û∞ÇØ {status}",
            "format-on": "Û∞ÇØ",
            "format-off": "Û∞ÇØ off",
            "format-disabled": "Û∞Ç≤",
            "format-no-controller": "Û∞Ç≤",
	    	"format-connected-battery": "{device_battery_percentage}% Û∞ÇØ",
            "tooltip-format": "{controller_alias}\t{controller_address}\n\n{num_connections} connected",
            "tooltip-format-connected": "{controller_alias}\t{controller_address}\n\n{num_connections} connected\n\n{device_enumerate}",
            "tooltip-format-enumerate-connected": "{device_alias}\n{device_address}",
            "tooltip-format-enumerate-connected-battery": "{device_alias}\n{device_address}\n{device_battery_percentage}%",
            "on-click-right": "wezterm start bluetui"
        }, 
        "custom/endpoint":{
            "format": " ",
            "tooltip": false
        },
        "group/tray": { 
            "orientation": "inherit", 
            "modules": ["custom/perf-icon", "cpu", "memory", "temperature"], 
        },
        "cpu": {
            "format": "{usage}% Ôíº",
            "tooltip": true
        },
        "memory": {
            "format": "{percentage}% ÓøÖ",
            "tooltip": true
		},
        "temperature": {
            "critical-threshold": 80,
            "format": "{temperatureC}¬∞ Ôãâ",
		    "hwmon-path": "/sys/devices/pci0000:00/0000:00:18.3/hwmon/hwmon1/temp1_input",
            "tooltip-format": "CPU: {temperatureC}¬∞C"
		},
        "tray": {
            "icon-size": 14,
            "spacing": 15
        },
		"pulseaudio#input": {
     		"scroll-step": 1,
        	"format": "{format_source}",
        	"format-source": "ÔÑ∞ {volume}%",
	        "format-source-muted": "ÔÑ± ",	
			"tooltip-format": "{format_source}",
    		"on-click": "pactl set-source-mute @DEFAULT_SOURCE@ toggle",	
		    "on-scroll-up": "pactl set-source-volume @DEFAULT_SOURCE@ +5%",
		    "on-scroll-down": "pactl set-source-volume @DEFAULT_SOURCE@ -5%"
		},
		"pulseaudio#output": {
     		"scroll-step": 1,
        	"format": "{icon} {volume}%",
	        "format-bluetooth": "Û∞Ç∞",
        	"format-bluetooth-muted": "{format_muted}",
	        "format-muted": "ÔÄ¶",
        	"format-icons": {
       			"headphone": "ÔÄ•‚Äâ",
            	"headset": "Óπô‚Äâ",
       	    	"default": ["ÔÄß", "ÔÄß", "ÔÄ® ", "ÔÄ® "]
		    },
			"tooltip-format": "{desc}",
	        "on-click": "pactl set-sink-mute @DEFAULT_SINK@ toggle",
            "on-click-right": "pavucontrol",
		},
    "custom/battery": {
        "exec": "~/.config/waybar/battery.sh",
        "return-type": "json",
        "format": "{icon} :  {percentage}%",
        "interval": 10,
        "tooltip": true,
        "format-icons": {
                "charging": "ÔÉß",
                "full": "ÔÉß",
                "discharging": [
                    "ÔâÑ",  // 0‚Äì29%
                    "ÔâÉ",  // 30‚Äì49%
                    "ÔâÇ",  // 50‚Äì69%
                    "ÔâÅ",  // 70‚Äì89%
                    "ÔâÄ"   // 90‚Äì100%
        ]
    }
},
"custom/pomodoro": {
	"format": "{}",
	"return-type": "json",
	"exec": "bash -c '$HOME/.local/bin/waybar-module-pomodoro'",
	"on-click": "bash -c '$HOME/.local/bin/waybar-module-pomodoro toggle'",
	"on-click-right": "bash -c '$HOME/.local/bin/waybar-module-pomodoro reset'",
}
} 
