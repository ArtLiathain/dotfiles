        "reload_style_on_change": true,
        "modules-left": ["custom/arch-icon","custom/separator", "hyprland/workspaces", "custom/separator","group/tray", "custom/separator", "custom/pomodoro"],
        "modules-center": ["clock"],
        "modules-right": [ "mpris","custom/separator", "privacy", "group/audio","custom/separator", "group/connection","custom/separator", "custom/battery"],
        "hyprland/workspaces": {
